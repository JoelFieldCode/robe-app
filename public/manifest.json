{
  "manifest_version": 2,
  "name": "Robe Extension",
  "author": "Joel Field",
  "version": "2.0.0",
  "description": "Add items to your wardrobe",
  "icons": {
    "16": "16.png",
    "48": "16.png",
    "128": "16.png"
  },
  "key": "",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "oauth2": {
    "client_id": "50857805831-3b2bsbrfbmk6u8vm0fthmd6svj1ebsn9.apps.googleusercontent.com",
    "scopes": ["profile", "openid"]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "identity",
    "identity.email",
    "http://localhost",
    "https://laravel-robe-api.herokuapp.com"
  ]
}
